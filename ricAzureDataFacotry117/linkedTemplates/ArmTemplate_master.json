{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ricAzureDataFacotry117"
		},
		"liked_service_on_prem_file_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'liked_service_on_prem_file'"
		},
		"linked_service_ADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'linked_service_ADLS'"
		},
		"Linked_service_github_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"liked_service_on_prem_file_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Users\\riccardo.petrella"
		},
		"liked_service_on_prem_file_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "riccardo.petrella@dolcegabbana.it"
		},
		"linked_service_ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfrickystorage.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ricAzureDataFacotry117_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"linked_service_ADLS_accountKey": {
						"value": "[parameters('linked_service_ADLS_accountKey')]"
					},
					"liked_service_on_prem_file_password": {
						"value": "[parameters('liked_service_on_prem_file_password')]"
					},
					"Linked_service_github_properties_typeProperties_url": {
						"value": "[parameters('Linked_service_github_properties_typeProperties_url')]"
					},
					"linked_service_ADLS_properties_typeProperties_url": {
						"value": "[parameters('linked_service_ADLS_properties_typeProperties_url')]"
					},
					"liked_service_on_prem_file_properties_typeProperties_host": {
						"value": "[parameters('liked_service_on_prem_file_properties_typeProperties_host')]"
					},
					"liked_service_on_prem_file_properties_typeProperties_userId": {
						"value": "[parameters('liked_service_on_prem_file_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}